      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react-native",
      "lib": ["esnext"],
      "module":"esnext",
      "moduleResolution": "node",
      "noEmit": true,
